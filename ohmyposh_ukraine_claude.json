          "template": " \uf489 {{ .CurrentDate | date \"Monday\" }} ",
          "type": "time"
        },
        {
          "background": "#FBD951",
          "foreground": "#0057b7",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \uf0c0 {{ .Env.USER }} ",
